this repository contains the full copyright notices and license terms. -->
<tryton>
    <data>
        <!-- aeat.rege -->
            <!-- Access -->
        <record model="ir.model.access" id="access_aeat_rege">
            <field name="model">aeat.rege</field>
            <field name="perm_read" eval="False"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_aeat_rege_group_company_admin">
            <field name="model">aeat.rege</field>
            <field name="group" ref="company.group_company_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>

            <!-- Views -->
        <record model="ir.ui.view" id="view_aeat_rege_form">
            <field name="model">aeat.rege</field>
            <field name="type">form</field>
            <field name="name">rege_form</field>
        </record>
        <record model="ir.ui.view" id="view_aeat_rege_list">
            <field name="model">aeat.rege</field>
            <field name="type">tree</field>
            <field name="name">rege_list</field>
        </record>

            <!-- Actions -->
        <record model="ir.action.act_window" id="act_aeat_rege">
            <field name="name">Special Regime for Groups of Entities</field>
            <field name="res_model">aeat.rege</field>
        </record>
        <record model="ir.action.act_window.view" id="act_aeat_rege_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="view_aeat_rege_form"/>
            <field name="act_window" ref="act_aeat_rege" />
        </record>
        <record model="ir.action.act_window.view" id="act_aeat_rege_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="view_aeat_rege_list"/>
            <field name="act_window" ref="act_aeat_rege" />
        </record>

            <!-- Menus -->
        <menuitem id="menu_aeat_rege" parent="company.menu_company" sequence="30" icon="tryton-list" action="act_aeat_rege" />

        <!-- aeat.rege.period -->
            <!-- Views -->
        <record model="ir.ui.view" id="view_aeat_rege_period_form">
            <field name="model">aeat.rege.period</field>
            <field name="type">form</field>
            <field name="name">rege_period_form</field>
        </record>
        <record model="ir.ui.view" id="view_aeat_rege_period_list">
            <field name="model">aeat.rege.period</field>
            <field name="type">tree</field>
            <field name="name">rege_period_list</field>
        </record>

            <!-- Actions -->
        <record model="ir.action.act_window" id="act_aeat_rege_period">
            <field name="name">REGE Periods</field>
            <field name="res_model">aeat.rege.period</field>
        </record>
        <record model="ir.action.act_window.view" id="act_aeat_rege_period_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="view_aeat_rege_period_form"/>
            <field name="act_window" ref="act_aeat_rege_period" />
        </record>
        <record model="ir.action.act_window.view" id="act_aeat_rege_period_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="view_aeat_rege_period_list"/>
            <field name="act_window" ref="act_aeat_rege_period" />
        </record>

            <!-- Menus -->
        <menuitem id="menu_aeat_rege_period" parent="menu_aeat_rege" sequence="10" icon="tryton-list" action="act_aeat_rege_period" />

        <!-- aeat.rege.member -->
            <!-- Views -->
        <record model="ir.ui.view" id="view_aeat_rege_member_form">
            <field name="model">aeat.rege.member</field>
            <field name="type">form</field>
            <field name="name">rege_member_form</field>
        </record>
        <record model="ir.ui.view" id="view_aeat_rege_member_list">
            <field name="model">aeat.rege.member</field>
            <field name="type">tree</field>
            <field name="name">rege_member_list</field>
        </record>

        <!-- Party (party.party) -->
        <record model="ir.ui.view" id="view_party_form">
            <field name="model">party.party</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="name">party_form</field>
        </record>

        <!-- InvoiceLine (account.invoice.line) -->
        <record model="ir.ui.view" id="invoice_line_view_form">
            <field name="model">account.invoice.line</field>
            <field name="inherit" ref="account_invoice.invoice_line_view_form"/>
            <field name="name">invoice_line_form</field>
        </record>
        <record model="ir.ui.view" id="invoice_line_view_tree">
            <field name="model">account.invoice.line</field>
            <field name="inherit" ref="account_invoice.invoice_line_view_tree"/>
            <field name="name">invoice_line_list</field>
        </record>
        <record model="ir.ui.view" id="invoice_line_view_tree_sequence">
            <field name="model">account.invoice.line</field>
            <field name="inherit" ref="account_invoice.invoice_line_view_tree_sequence"/>
            <field name="name">invoice_line_list_sequence</field>
        </record>

        <!-- aeat.rege -->
            <!-- Related Records -->
                <!-- Members -->
        <record model="ir.action.act_window" id="act_aeat_rege_members_related">
            <field name="name">REGE Members</field>
            <field name="res_model">aeat.rege.member</field>
            <field name="domain" eval="[('rege', 'in', Eval('active_ids'))]" pyson="1"/>
        </record>

        <record model="ir.action.act_window.domain" id="act_aeat_rege_members_related_active_domain">
            <field name="name">Today Active</field>
            <field name="sequence" eval="10"/>
            <field name="domain" eval="[('is_active', '=', True)]" pyson="1"/>
            <field name="count" eval="True"/>
            <field name="act_window" ref="act_aeat_rege_members_related"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_aeat_rege_members_related_all_domain">
            <field name="name">All</field>
            <field name="sequence" eval="10"/>
            <field name="domain" eval="[]" pyson="1"/>
            <field name="act_window" ref="act_aeat_rege_members_related"/>
        </record>

        <record model="ir.action.act_window.view" id="act_aeat_rege_members_related_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="view_aeat_rege_member_list"/>
            <field name="act_window" ref="act_aeat_rege_members_related"/>
        </record>
        <record model="ir.action.act_window.view" id="act_aeat_rege_members_related_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="view_aeat_rege_member_form"/>
            <field name="act_window" ref="act_aeat_rege_members_related"/>
        </record>
        <record model="ir.action.keyword" id="act_aeat_rege_members_related_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">aeat.rege,-1</field>
            <field name="action" ref="act_aeat_rege_members_related"/>
        </record>
    </data>
</tryton>