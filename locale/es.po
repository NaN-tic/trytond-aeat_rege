msgctxt "model:ir.message,text:msg_date_interval_period"
msgid "The \"Start Date\" must be earlier than the \"End Date\"."
msgstr "La \"Fecha inicial\" debe ser anterior a la \"Fecha final\"."

msgctxt "model:ir.message,text:msg_date_interval_registration"
msgid "The \"Registration Date\" must be earlier than the \"Exit Date\"."
msgstr "La \"Fecha de registro\" debe ser anterior a la \"Fecha de salida\"."

msgctxt "model:ir.message,text:msg_membership_overlap"
msgid ""
"The membership \"%(main)s\" overlaps with the following membership(s): "
"\"%(member)s\". Please adjust the date ranges to avoid overlap."
msgstr ""
"La membresía \"%(main)s\" se superpone con la(s) siguiente(s) membresía(s): "
"\"%(member)s\". Ajuste los rangos de fechas para evitar solapamientos."

msgctxt "model:ir.message,text:msg_period_overlap"
msgid ""
"The period \"%(main)s\" overlaps with the following period(s): "
"\"%(period)s\". Please adjust the date ranges to avoid overlap."
msgstr ""
"El período \"%(main)s\" se superpone con el/los siguiente(s) período(s): "
"\"%(period)s\". Ajuste los rangos de fechas para evitar solapamientos."

msgctxt "model:ir.message,text:msg_unique_rege_name"
